meta {
  name: graphQL
  type: graphql
  seq: 3
}

post {
  url: https://rickandmortyapi.com/graphql
  body: graphql
  auth: inherit
}

headers {
  Content-Type: application/json
}

body:graphql {
  {
    characters(page: 1) {
      results {
        name
        species
        status
      }
    }
  }
  
}

settings {
  encodeUrl: true
  timeout: 0
}
